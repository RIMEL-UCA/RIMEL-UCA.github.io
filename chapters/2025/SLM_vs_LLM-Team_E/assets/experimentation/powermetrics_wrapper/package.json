  "license": "ISC",
  "dependencies": {
    "express": "^4.21.2",
    "fast-xml-parser": "^4.5.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.10",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
