{
  "name": "powermetrics_wrapper",
  "version": "1.0.0",
  "description": "macOS powermetrics wrapper enabling to collect data",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon --watch src -e js,ts,json --exec \"ts-node src/index.ts\"",
    "build": "tsup src/index.ts --format cjs --clean --minify",
    "start": "node dist/index.js"
  },
